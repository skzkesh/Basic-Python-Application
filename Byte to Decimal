def byte_to_decimal(b7, b6, b5, b4, b3, b2, b1, b0):
    """
    Parameters
    ----------
    b7, ..., b0: single characters either '0' or '1'. 
    """
    # YOUR CODE HERE
    decimal = int(b7) * (2 ** 7) + int(b6) * (2 ** 6) + int(b5) * (2 ** 5) + int(b4) * (2 ** 4) + int(b3) * (2 ** 3) + int(b2) * (2 ** 2) + int(b1) * (2 ** 1) + int(b0) * (2 ** 0)
    return decimal
